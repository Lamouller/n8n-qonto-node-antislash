{
  "name": "@antislash/n8n-nodes-qonto-antislash",
  "version": "1.1.4",
  "description": "Qonto is a neo-bank. This node gives access to all functions of this bank",
  "keywords": ["n8n-community-node-package", "n8n", "qonto", "integration"],
  "license": "MIT",
  "homepage": "https://github.com/DtNeo/n8n-qonto-node#readme",
  "author": {
    "name": "Antislash",
    "email": "trystan@antislash.studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DtNeo/n8n-qonto-node.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "lint": "tslint -p tsconfig.json -c tslint.json && eslint nodes credentials package.json",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": ["dist"],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/QontoApi.credentials.js",
      "dist/credentials/QontoOAuth2Api.credentials.js"
    ],
    "nodes": ["dist/nodes/Qonto/Qonto.node.js"]
  },
  "devDependencies": {
    "typescript": "~4.6.0",
    "tslint": "^6.1.2",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0"
  }
}
